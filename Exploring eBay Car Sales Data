    "\n",
    "#Convert mean_mileages and mean_prices dictionaries into pandas Series objects\n",
    "mean_prices_series = pd.Series(mean_prices)\n",
    "mean_mileages_series = pd.Series(mean_mileages)\n",
    "\n",
    "#Make a dataframe of both of these series\n",
    "df = pd.DataFrame(mean_prices_series, columns = ['mean_price'])\n",
