    "mean_prices = {}\n",
    "top_7_autos = [\"volkswagen\", \"opel\", \"bmw\", \"mercedes_benz\", \"audi\", \"ford\", \"renault\"]\n",
    "for brand_name in top_7_autos:\n",
    "    mean_prices[brand_name] = cleaned_autos.loc[cleaned_autos[\"brand\"] == brand_name, \"price\"].mean()\n",
    "print(mean_prices)"
